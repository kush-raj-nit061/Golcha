<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".VendorPDF"
    android:scrollbars="none">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <!-- invoice status -->
        <TextView android:id="@+id/invoice_status"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:background="#E2E2E2"
            android:textColor = "@color/carbon_pink_900"
            android:textStyle="bold"
            android:padding = "10dp"
            android:text="Invoice" />
        <!-- end invoice status -->
        <!-- invoice/client data -->
        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop = "3dip"
            android:paddingRight = "3dip"
            android:paddingLeft = "3dip"
            android:paddingBottom="24dip"
            android:stretchColumns="1">
            <TableRow>
                <TextView android:id="@+id/invoice_name"
                    android:text="@string/sasa"
                    android:textStyle="bold"
                    android:padding="3dip" />
                <TextView android:id="@+id/vendorNames"
                    android:text=""
                    android:padding="3dip" />
                <TextView android:id="@+id/invoice_date"
                    android:text="@string/date"
                    android:textStyle="bold"
                    android:textColor="@color/purple_700"
                    android:gravity="right"
                    android:padding="3dip"
                    tools:ignore="RtlHardcoded" />
            </TableRow>
            <TableRow>
                <TextView android:id="@+id/invoice_client"
                    android:text="@string/address"
                    android:textStyle="bold"
                    android:paddingStart="3dip"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" />
                <TextView android:id="@+id/vendorAddresss"
                    android:text=""
                    android:paddingStart="3dip"
                    android:paddingTop="5dip"
                    android:textSize = "45px"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content" />
            </TableRow>


        </TableLayout>
        <!-- end invoice/client data -->
        <!-- invoice items -->
        <TableLayout
            android:layout_width="fill_parent"
